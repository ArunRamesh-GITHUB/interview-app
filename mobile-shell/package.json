{
  "name": "mobile-shell",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build:android": "eas build -p android --profile production",
    "build:ios": "eas build -p ios --profile production",
    "build:android:preview": "eas build -p android --profile preview",
    "build:ios:preview": "eas build -p ios --profile preview",
    "submit:android": "eas submit -p android --latest",
    "submit:ios": "eas submit -p ios --latest",
    "doctor": "npx expo-doctor"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "expo": "~53.0.23",
    "expo-av": "^15.1.7",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.7",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "react-native-branch": "^6.8.0",
    "react-native-iap": "^13.0.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/cli": "^0.24.21",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
